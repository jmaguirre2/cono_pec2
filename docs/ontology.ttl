:City rdf:type owl:Class ;
      rdfs:comment "Cities where tourism events take place"@en ;
      rdfs:label "City"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Cuisine
:Cuisine rdf:type owl:Class ;
         rdfs:subClassOf :Restaurant ;
         rdfs:comment "Types of restaurants by type of food"@en ;
         rdfs:label "Cuisine"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Cultural
:Cultural rdf:type owl:Class ;
          rdfs:subClassOf :Event ;
          rdfs:comment "Cultural events"@en ;
          rdfs:label "Cultural"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Culture
:Culture rdf:type owl:Class ;
         rdfs:subClassOf :Attraction ;
         rdfs:comment "Cultural attractions"@en ;
         rdfs:label "Culture"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Event
:Event rdf:type owl:Class ;
       rdfs:comment "Types of events that tourists can enjoy in cities"@en ;
       rdfs:label "Event"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Gastronomic
:Gastronomic rdf:type owl:Class ;
             rdfs:subClassOf :Event ;
             rdfs:comment "Gastronomic events"@en ;
             rdfs:label "Gastronomic"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#High
:High rdf:type owl:Class ;
      rdfs:subClassOf :Range ;
      rdfs:comment "High range of a hotel"@en ;
      rdfs:label "High"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :has_stars ;
                         owl:someValuesFrom :Starts
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :rate_hotel ;
                         owl:someValuesFrom :Rating
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_stars ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Starts
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :rate_hotel ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Rating
                       ] ;
       rdfs:comment "Types of hotels where tourists can stay during their visit to the city."@en ;
       rdfs:label "Hotel"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Italian
:Italian rdf:type owl:Class ;
         rdfs:subClassOf :Cuisine ;
         rdfs:comment "Italian food restaurant"@en ;
         rdfs:label "Italian"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Japanese
:Japanese rdf:type owl:Class ;
          rdfs:subClassOf :Cuisine ;
          rdfs:comment "Japanese food restaurant"@en ;
          rdfs:label "Japanese"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Land
:Land rdf:type owl:Class ;
      rdfs:subClassOf :Transport ;
      rdfs:comment "Land transport"@en ;
      rdfs:label "Land"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Low
:Low rdf:type owl:Class ;
     rdfs:subClassOf :Range ;
     rdfs:comment "Low range of a hotel"@en ;
     rdfs:label "Low"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Luxury
:Luxury rdf:type owl:Class ;
        rdfs:subClassOf :Hotel ;
        rdfs:comment "Luxury hotels"@en ;
        rdfs:label "Luxury"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Mediterranean
:Mediterranean rdf:type owl:Class ;
               rdfs:subClassOf :Cuisine ;
