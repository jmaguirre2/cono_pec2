#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#Rate
:Rate rdf:type rdfs:Datatype ;
      owl:equivalentClass :Rate .

[ rdf:type owl:Axiom ;
   owl:annotatedSource :Rate ;
   owl:annotatedProperty owl:equivalentClass ;
   owl:annotatedTarget :Rate ;
   rdfs:comment _:genid2
 ] .

_:genid2 :rate """DatatypeDefinition(
   :rate
   DatatypeRestriction( xsd:integer           
     xsd:minInclusive \"0\"^^xsd:integer
     xsd:maxInclusive \"5\"^^xsd:integer
   )
 )"""^^xsd:Integer .


###  http://www.w3.org/2001/XMLSchema#Integer
xsd:Integer rdf:type rdfs:Datatype .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/latitude
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/latitude> rdf:type rdfs:Datatype .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/longitude
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/longitude> rdf:type rdfs:Datatype .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/lat/long
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/lat/long> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#isTypeOf
:isTypeOf rdf:type owl:ObjectProperty ;
          rdfs:domain :ElementType ;
          rdfs:range :Attraction ,
                     :Event ,
                     :Hotel ,
                     :Restaurant ,
                     :Transport .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#locatedIn
:locatedIn rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain [ owl:intersectionOf ( :Attraction
                                              :Event
                                              :Hotel
                                              :Restaurant
                                              :Transport
                                            ) ;
                         rdf:type owl:Class
                       ] ;
           rdfs:range :City .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#hasAttractionId
:hasAttractionId rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :id ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Attraction ,
                             :City ;
                 rdfs:range xsd:Integer .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#hasAttractionName
:hasAttractionName rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :name ;
                   rdfs:domain :Attraction ;
                   rdfs:range xsd:string .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#hasAttractionRate
:hasAttractionRate rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :rate ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :Attraction ;
                   rdfs:range :Rate ;
