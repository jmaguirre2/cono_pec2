       rdfs:label "value"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#valuestart
:valuestart rdf:type owl:DatatypeProperty ;
            rdfs:domain :Starts ;
            rdfs:range xsd:int ;
            rdfs:comment "Value of Start"@en ;
            rdfs:label "valuestart"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Andalusian
:Andalusian rdf:type owl:Class ;
            rdfs:subClassOf :Cuisine ;
            rdfs:comment "Andalusian food restaurant"@en ;
            rdfs:label "Andalusian"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :rate_attraction ;
                              owl:someValuesFrom :Rating
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :rate_attraction ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Rating
                            ] ;
            rdfs:comment "Indicates possible attractions in the cities"@en ;
            rdfs:label "Attraction"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Beach
:Beach rdf:type owl:Class ;
       rdfs:subClassOf :Attraction ;
       rdfs:comment "Attractions located on the beach"@en ;
       rdfs:label "Beach"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Bicycle
:Bicycle rdf:type owl:Class ;
         rdfs:subClassOf :Land ;
         rdfs:comment "Use the bicycle as transport"@en ;
         rdfs:label "Bicycle"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Land ;
     rdfs:comment "Use the bus as transport"@en ;
     rdfs:label "Bus"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :Hotel ;
          rdfs:comment "Business hotels"@en ;
          rdfs:label "Business"@en .


