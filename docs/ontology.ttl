#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2003/01/geo/wgs84_pos#lat_long
<http://www.w3.org/2003/01/geo/wgs84_pos#lat_long> rdf:type rdfs:Datatype ;
                                                   rdfs:comment "A comma-separated representation of a latitude, longitude coordinate."@en ;
                                                   rdfs:label "lat_long"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#location
<http://www.w3.org/2003/01/geo/wgs84_pos#location> rdf:type rdfs:Datatype ;
                                                   rdfs:comment "The relation between something and the point, or other geometrical thing in space, where it is. For example, the realtionship between a radio tower and a Point with a given lat and long. Or a relationship between a park and its outline as a closed arc of points, or a road and its location as a arc (a sequence of points). Clearly in practice there will be limit to the accuracy of any such statement, but one would expect an accuracy appropriate for the size of the object and uses such as mapping ." ;
                                                   rdfs:label "location"@en .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#has_attractions
:has_attractions rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ;
                 rdfs:domain :City ;
                 rdfs:range :Attraction ;
                 rdfs:comment "This object property facilitates a city's attractions"@en ;
                 rdfs:label "has_attractions"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#has_events
:has_events rdf:type owl:ObjectProperty ,
                     owl:AsymmetricProperty ;
            rdfs:domain :City ;
            rdfs:range :Event ;
            rdfs:comment "This object property facilitates the events of a city."@en ;
            rdfs:label "has_events"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#has_hotels
:has_hotels rdf:type owl:ObjectProperty ,
                     owl:AsymmetricProperty ;
            rdfs:domain :City ;
            rdfs:range :Hotel ;
            rdfs:comment "This object property facilitates the hotels in a city"@en ;
            rdfs:label "has_hotels"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#has_restaurants
:has_restaurants rdf:type owl:ObjectProperty ,
                          owl:AsymmetricProperty ;
                 rdfs:domain :City ;
                 rdfs:range :Restaurant ;
                 rdfs:comment "This object property facilitates the restaurants of a city."@en ;
                 rdfs:label "has_restaurants"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#has_stars
:has_stars rdf:type owl:ObjectProperty ;
           rdfs:domain :Hotel ;
           rdfs:range :Starts ;
           rdfs:comment "This object property provides the stars of a hotel"@en ;
           rdfs:label "has_starts"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#has_transports
:has_transports rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain :City ;
                rdfs:range :Transport ;
                rdfs:comment "This object property facilitates the transports of a city."@en ;
                rdfs:label "has_transports"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#operated_by
:operated_by rdf:type owl:ObjectProperty ;
             rdfs:domain :Transport ;
             rdfs:range :TransportCompany ;
             rdfs:comment "This object property provides the company by which a transport is operated."@en ;
             rdfs:label "operated_by"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#rate_attraction
:rate_attraction rdf:type owl:ObjectProperty ;
                 rdfs:domain :Attraction ;
                 rdfs:range :Rating ;
                 rdfs:comment "This property of object facilitates the valuation of an attraction"@en ;
                 rdfs:label "rate_attraction"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#rate_hotel
:rate_hotel rdf:type owl:ObjectProperty ;
            rdfs:domain :Hotel ;
            rdfs:range :Rating ;
            rdfs:comment "This property of object facilitates the valuation of a hotel"@en ;
            rdfs:label "rate_hotel"@en .


###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#rate_restaurant
:rate_restaurant rdf:type owl:ObjectProperty ;
                 rdfs:domain :Restaurant ;
                 rdfs:range :Rating ;
                 rdfs:comment "Esta propiedad de objeto facilita la valoración de un restaurante"@en ;
                 rdfs:label "rate_restaurant"@en .


