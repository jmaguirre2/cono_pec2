###  http://www.semanticweb.org/kowalski333/ontologies/2025/3/tourism#name_start
:name_start rdf:type owl:DatatypeProperty ;
            rdfs:domain :Starts ;
            rdfs:range xsd:string ;
            rdfs:comment "Identifying name of Start"@en ;
