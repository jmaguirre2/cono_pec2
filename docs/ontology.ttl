###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#id
:id rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdf:type owl:FunctionalProperty ;
    rdfs:domain [ owl:intersectionOf ( :Attraction
                                       :City
                                       :Event
                                       :Hotel
                                       :Restaurant
                                       :Transport
                                     ) ;
                  rdf:type owl:Class
                ] ;
    rdfs:range xsd:integer .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#location
:location rdf:type owl:DatatypeProperty ;
          rdfs:domain :City ;
          rdfs:range <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/latitude> ,
                     <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing/lat/long> .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#name
:name rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range xsd:string .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#price
:price rdf:type owl:DatatypeProperty ;
       rdfs:domain :Hotel ,
                   :Restaurant ,
                   :Transport ;
       rdfs:range xsd:decimal .


###  http://www.semanticweb.org/manolito/ontologies/2025/3/Smart_Tourism_and_Recommendations#rate
